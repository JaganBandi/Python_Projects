pipeline{
	agent any
	stages{
	stage('checkout'){
	git url:'https://github.com/user/repo.git',branch:'main'
		}
	}
	stage('build'){
	stage{
	sh 'mvn clean storage'
	}
	}
	stage('test'){
	stage{
	sh 'mvn test'
	}
	}
}
post{
	success{
	echo'pipeline completed successfully'
	}
	failure{
	echo'pipeline failed'
	}
}